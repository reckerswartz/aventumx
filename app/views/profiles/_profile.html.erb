<div id="<%= dom_id profile %>">
  <p>
    <strong>Email:</strong>
    <%= profile.email %>
  </p>

  <p>
    <strong>Birthday:</strong>
    <%= profile.birthday %>
  </p>

  <p>
    <strong>Occupation:</strong>
    <%= profile.occupation %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= profile.status %>
  </p>

  <p>
    <strong>Birthplace:</strong>
    <%= profile.birthplace %>
  </p>

  <p>
    <strong>Web:</strong>
    <%= profile.web %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= profile.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= profile.last_name %>
  </p>

</div>
